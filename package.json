  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^5.10.14",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
